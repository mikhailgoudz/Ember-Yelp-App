<nav class="navbar navbar-expand-lg navbar-light" style="background-color: #e3f2fd;">
  <a class="navbar-brand">Yelp Pages</a>
  
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
      {{#if session.isAuthenticated}}
      <span class="nav-item nav-link"> <LinkTo @route = "index">Search</LinkTo><span class="sr-only">(current)</span></span>
      <span class="nav-item nav-link"> <LinkTo @route = "favorites">Favorites</LinkTo></span>
      <span class="nav-item nav-link"> <span> Logged in as </span>{{ session.data.authenticated.user.displayName }}
  <button  {{action "logout"}}>Sign out</button></span>

      {{else}}
           <span class="nav-item nav-link">  <button class = "btn btn-primary" {{action "login"}}>Sign in with Google</button> </span>
      {{/if}}
      
    </div>
  </div>
</nav>

